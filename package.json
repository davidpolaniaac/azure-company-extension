{
  "name": "azure-devops-company",
  "version": "1.0.0",
  "description": "Azure DevOps Company extension",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidpolaniaac/azure-company-extension.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "build": "npm run compile",
    "build:dev": "npm run compile:dev && npm run postbuild",
    "postbuild": "npm run package-extension -- --output-path dist --rev-version",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json src/extensions/**/*.json",
    "publish-extension": "tfx extension publish --manifest-globs azure-devops-extension.json src/extensions/**/*.json"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "azure-devops-extension-api": "~1.150.1",
    "azure-devops-extension-sdk": "~2.0.6",
    "azure-devops-ui": "~1.150.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-form-validators": "^2.7.5",
    "redux-recompose": "^2.1.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-eslint": "9.0.0",
    "babel-runtime": "6.26.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "~1.0.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "file-loader": "~2.0.0",
    "node-sass": "^4.11.0",
    "redux-devtools": "^3.5.0",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "tfx-cli": "^0.6.3",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
